{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "types": ["webpack-env", "node"],
    "lib": ["ES2015", "DOM"],
    "paths": {
      "@/*": ["src/*"]
    },
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "src/**/*" , "src/settings/types/global.d.ts", "src/setting/types/env.d.ts" ],
}